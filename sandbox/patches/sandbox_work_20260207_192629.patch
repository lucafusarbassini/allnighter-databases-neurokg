+        edge_count = 0
+        status = "OK"
+        node_trunc = False
+        edge_trunc = False
+
+        try:
+            # Import the module
+            mod = importlib.import_module(module_path)
+            cls = getattr(mod, class_name)
+            adapter = cls()
+
+            # Count nodes
+            try:
+                node_gen = adapter.get_nodes()
+                node_count, node_trunc = count_iterator(node_gen)
+            except Exception as e:
+                status = f"node_err: {str(e)[:60]}"
+
+            # Count edges
+            try:
+                edge_gen = adapter.get_edges()
+                edge_count, edge_trunc = count_iterator(edge_gen)
+            except Exception as e:
+                if status == "OK":
+                    status = f"edge_err: {str(e)[:60]}"
+                else:
+                    status += f" | edge_err: {str(e)[:60]}"
+
+            if node_trunc or edge_trunc:
+                parts = []
+                if node_trunc:
+                    parts.append("nodes_truncated")
+                if edge_trunc:
+                    parts.append("edges_truncated")
+                if status == "OK":
+                    status = ", ".join(parts)
+                else:
+                    status += " | " + ", ".join(parts)
+
+        except Exception as e:
+            status = f"LOAD_ERR: {str(e)[:80]}"
+            errors += 1
+
+        elapsed = time.time() - t0
+        total_nodes += node_count
+        total_edges += edge_count
+
+        node_str = f"{node_count:,}" if node_count > 0 else "0"
+        edge_str = f"{edge_count:,}" if edge_count > 0 else "0"
+        total_str = f"{node_count + edge_count:,}"
+
+        line = f"{idx:<4} {name:<22} {node_str:>12} {edge_str:>12} {total_str:>12} {elapsed:>7.1f}s {status}"
+        print(line, flush=True)
+        results.append((name, node_count, edge_count, elapsed, status))
+
+    print(sep)
+    print()
+
+    # Summary
+    grand_total = total_nodes + total_edges
+    working = sum(1 for _, n, e, _, s in results if (n + e) > 0)
+    failed = sum(1 for _, n, e, _, s in results if "ERR" in s.upper())
+    empty = sum(1 for _, n, e, _, s in results if (n + e) == 0 and "ERR" not in s.upper())
+
+    print("=" * 100)
+    print("SUMMARY")
+    print("=" * 100)
+    print(f"Total adapters:       {len(ADAPTERS)}")
+    print(f"Working (>0 records): {working}")
+    print(f"Empty (0 records):    {empty}")
+    print(f"Failed (errors):      {failed}")
+    print(f"Total nodes:          {total_nodes:,}")
+    print(f"Total edges:          {total_edges:,}")
+    print(f"Grand total:          {grand_total:,}")
+    print()
+
+    # Top 20 by total records
+    sorted_results = sorted(results, key=lambda x: x[1] + x[2], reverse=True)
+    print("TOP 20 ADAPTERS BY TOTAL RECORDS:")
+    print(f"{'Rank':<6} {'Adapter':<22} {'Nodes':>12} {'Edges':>12} {'Total':>12}")
+    print("-" * 70)
+    for rank, (name, nc, ec, _, _) in enumerate(sorted_results[:20], 1):
+        print(f"{rank:<6} {name:<22} {nc:>12,} {ec:>12,} {nc+ec:>12,}")
+
+    print()
+
+    # List adapters with errors
+    err_adapters = [(name, status) for name, _, _, _, status in results if "ERR" in status.upper() or "err" in status]
+    if err_adapters:
+        print("ADAPTERS WITH ERRORS:")
+        print("-" * 70)
+        for name, status in err_adapters:
+            print(f"  {name}: {status}")
+        print()
+
+    # List empty adapters
+    empty_adapters = [name for name, n, e, _, s in results if (n + e) == 0 and "ERR" not in s.upper() and "err" not in s]
+    if empty_adapters:
+        print("ADAPTERS WITH ZERO RECORDS (no errors):")
+        print("-" * 70)
+        for name in empty_adapters:
+            print(f"  {name}")
+        print()
+
+
+if __name__ == "__main__":
+    main()
