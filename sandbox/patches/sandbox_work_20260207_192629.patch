+RECORD_LIMIT = 5_000_000  # Max records to count per method before stopping
+
+def count_iterator(gen, limit=RECORD_LIMIT):
+    """Count items from a generator, up to limit."""
+    count = 0
+    truncated = False
+    for _ in gen:
+        count += 1
+        if count >= limit:
+            truncated = True
+            break
+    return count, truncated
+
+
+def main():
+    import importlib
+
+    results = []
+    total_nodes = 0
+    total_edges = 0
+    errors = 0
+
+    header = f"{'#':<4} {'Adapter':<22} {'Nodes':>12} {'Edges':>12} {'Total':>12} {'Time':>8} {'Status'}"
+    sep = "-" * 100
+
+    print("=" * 100)
+    print("BioCypher Adapter Record Count")
+    print(f"Date: {time.strftime('%Y-%m-%d %H:%M:%S')}")
+    print(f"Record limit per method: {RECORD_LIMIT:,}")
+    print("=" * 100)
+    print()
+    print(header)
+    print(sep)
+
+    for idx, (name, module_path, class_name) in enumerate(ADAPTERS, 1):
+        t0 = time.time()
